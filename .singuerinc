#!/bin/bash
alias j="autojump"
alias ports="sudo lsof -i -P"
alias sub="subliminal download -l es"
alias suben="subliminal download -l en"

alias d="docker"
alias dps="docker ps"

docker_environment(){
    docker-machine start $1
    eval "$(docker-machine env $1)"
}

alias denv=docker_environment

docker_machine_ip(){
    docker-machine ip $1
}

alias dip=docker_machine_ip

alias cls="clear"
alias rhc='docker run --rm -it --volumes-from rhc-data ecarre/rhc'
