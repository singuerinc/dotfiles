#!/bin/bash
alias j="autojump"
alias ports="sudo lsof -i -P"
alias sub="subliminal download -l es"
alias suben="subliminal download -l en"

alias dm="docker-machine"
alias dstart="docker start"
alias dstop="docker stop"
alias dexec="docker exec -t -i"
alias drm="docker rm -f"
alias drun="docker run --rm"


docker_environment(){
    docker-machine start $1
    eval "$(docker-machine env $1)"
}

alias denv=docker_environment

docker_machine_ip(){
    docker-machine ip $1
}

alias dip=docker_machine_ip
