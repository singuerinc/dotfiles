{
  "editor.fontFamily": "JetBrains Mono,Input,SFMono-Regular",
  "editor.renderWhitespace": "selection",
  "editor.fontWeight": "normal",
  "terminal.integrated.fontWeight": "500",
  "window.title": "${activeEditorShort}${separator}${activeEditorLong}",
  "editor.rulers": [],
  "files.trimTrailingWhitespace": true,
  "terminal.integrated.fontSize": 18,
  "editor.matchBrackets": "always",
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "npm.packageManager": "yarn",
  "explorer.openEditors.visible": 0,
  "terminal.integrated.cursorBlinking": true,
  "typescript.updateImportsOnFileMove.enabled": "never",
  "search.quickOpen.includeSymbols": true,
  "editor.renderControlCharacters": true,
  "workbench.activityBar.visible": false,
  "git.enableSmartCommit": true,
  "git.autofetch": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "window.zoomLevel": 0,
  "workbench.statusBar.visible": true,
  "breadcrumbs.enabled": false,
  "editor.minimap.enabled": true,
  "workbench.sideBar.location": "left",
  "editor.colorDecorators": true,
  "workbench.list.keyboardNavigation": "filter",
  "editor.suggestSelection": "first",
  "editor.fontLigatures": true,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.suggest.localityBonus": true,
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "emmet.includeLanguages": {
    "postcss": "css"
  },
  "emmet.syntaxProfiles": {
    "postcss": "css"
  },
  "explorer.decorations.badges": false,
  "bracket-pair-colorizer-2.colors": [
    "#63e6be",
    "#8ce99a",
    "#b197fc",
    "#f06595"
  ],
  "workbench.colorCustomizations": {
    "tab.activeBackground": "#111",
    "tab.activeForeground": "#fff",
    "[Tokyo Night]": {
      // or "[Tokyo Night Storm]"
      "window.inactiveBorder": "#222"
    }
  },
  "workbench.editor.showTabs": true,
  "vim.easymotion": true,
  "vim.incsearch": true,
  "vim.leader": "\\",
  "vim.easymotionMarkerBackgroundColor": "#00FF55cc",
  "vim.easymotionMarkerWidthPerChar": 10,
  "vim.easymotionMarkerHeight": 20,
  "vim.easymotionMarkerFontFamily": "Input",
  "vim.easymotionMarkerFontSize": "12px",
  "editor.autoClosingQuotes": "always",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "emmet.showSuggestionsAsSnippets": true,
  "emmet.triggerExpansionOnTab": true,
  "gitlens.codeLens.enabled": false,
  "gitlens.views.repositories.location": "gitlens",
  "gitlens.views.fileHistory.location": "gitlens",
  "gitlens.views.compare.location": "gitlens",
  "gitlens.views.search.location": "gitlens",
  "dimmer.context": 6,
  "dimmer.opacity": 10,
  "workbench.iconTheme": "origamid-next-icons",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.fontAliasing": "auto",
  "terminal.integrated.fontFamily": "Input,SFMono-Regular",
  "git.untrackedChanges": "separate",
  "editor.fontSize": 17,
  "workbench.colorTheme": "City Lights",
  "explorer.confirmDelete": false,
  "editor.formatOnSave": true,
  "editor.minimap.renderCharacters": false,
  "cSpell.userWords": [
    "Autoplay",
    "FWSUPPORT",
    "PAYTABLE",
    "PRODBUG",
    "QUICKSPIN",
    "SPACEBAR",
    "TOPBAR",
    "Quickspin",
    "SITEURL",
    "Valentina",
    "WORDPRESS",
    "aniket",
    "clienthost",
    "customizer",
    "garima",
    "netent",
    "placemat",
    "playui",
    "vova"
  ],
  "workbench.editor.highlightModifiedTabs": true,
  "editor.tabCompletion": "on",
  "problems.showCurrentInStatus": true,
  "terminal.integrated.rendererType": "experimentalWebgl",
  "search.enableSearchEditorPreview": true,
  "terminal.external.osxExec": "Hyper.app",
  "gitlens.blame.avatars": false,
  "editor.parameterHints.cycle": true,
  "editor.renderIndentGuides": false
}
