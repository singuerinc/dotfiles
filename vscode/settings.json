{
  "editor.fontFamily": "JetBrains Mono, Input, SFMono-Regular",
  "editor.renderWhitespace": "selection",
  "window.title": "${activeEditorShort}${separator}${activeEditorLong}",
  "editor.rulers": [{
    "column": 80,
    "color": "#00FF0011"
  },
  {
    "column": 100,
    "color": "#FF990011"
  },
  {
      "column": 120,
      "color": "#FF000011"
  }],
  "files.trimTrailingWhitespace": true,
  "terminal.integrated.fontSize": 18,
  "editor.matchBrackets": "always",
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "npm.packageManager": "yarn",
  "explorer.openEditors.visible": 0,
  "terminal.integrated.cursorBlinking": true,
  "typescript.updateImportsOnFileMove.enabled": "never",
  "search.quickOpen.includeSymbols": true,
  "editor.renderControlCharacters": true,
  "git.enableSmartCommit": true,
  "git.autofetch": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "window.zoomLevel": 0,
  "workbench.statusBar.visible": true,
  "breadcrumbs.enabled": false,
  "editor.minimap.enabled": false,
  "workbench.sideBar.location": "right",
  "editor.colorDecorators": true,
  "workbench.list.keyboardNavigation": "filter",
  "editor.suggestSelection": "first",
  "editor.fontLigatures": true,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.suggest.localityBonus": true,
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "emmet.includeLanguages": {
    "postcss": "css"
  },
  "emmet.syntaxProfiles": {
    "postcss": "css"
  },
  "explorer.decorations.badges": false,
  // "bracket-pair-colorizer-2.colors": [
  //   "#63e6be",
  //   "#8ce99a",
  //   "#b197fc",
  //   "#f06595"
  // ],
  "bracket-pair-colorizer-2.activeScopeCSS": [
    "borderStyle : solid",
    "borderWidth : 1px",
    "borderColor : {color}",
    "opacity: 0.5",
    "backgroundColor : {color}"
  ],
  "workbench.editor.showTabs": true,
  "vim.easymotion": true,
  "vim.incsearch": true,
  "vim.leader": "\\",
  "vim.easymotionMarkerBackgroundColor": "#00FF55cc",
  "vim.easymotionMarkerWidthPerChar": 10,
  "vim.easymotionMarkerHeight": 20,
  "vim.easymotionMarkerFontFamily": "Input",
  "vim.easymotionMarkerFontSize": "12px",
  "editor.autoClosingQuotes": "always",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "emmet.showSuggestionsAsSnippets": true,
  "emmet.triggerExpansionOnTab": true,
  "gitlens.codeLens.enabled": false,
  "gitlens.views.repositories.location": "gitlens",
  "gitlens.views.fileHistory.location": "gitlens",
  "gitlens.views.compare.location": "gitlens",
  "gitlens.views.search.location": "gitlens",
  "dimmer.context": 6,
  "dimmer.opacity": 10,
  "workbench.fontAliasing": "auto",
  "terminal.integrated.fontFamily": "JetBrains Mono, Input, SFMono-Regular",
  "git.untrackedChanges": "separate",
  "editor.fontSize": 16,
  "explorer.confirmDelete": false,
  "editor.minimap.renderCharacters": false,
  "cSpell.userWords": [
    "Autoplay",
    "DEVTOOLS",
    "FWSUPPORT",
    "PAYTABLE",
    "PRODBUG",
    "QUICKSPIN",
    "Quickspin",
    "SITEURL",
    "SPACEBAR",
    "TOPBAR",
    "Valentina",
    "WORDPRESS",
    "aniket",
    "clienthost",
    "customizer",
    "garima",
    "immer",
    "jira",
    "netent",
    "placemat",
    "playui",
    "singuerinc",
    "uuidv",
    "vova",
    "xstate"
  ],
  "workbench.editor.highlightModifiedTabs": true,
  "editor.tabCompletion": "on",
  "problems.showCurrentInStatus": true,
  "search.enableSearchEditorPreview": true,
  "terminal.external.osxExec": "Hyper.app",
  "gitlens.blame.avatars": false,
  "editor.parameterHints.cycle": true,
  "editor.renderIndentGuides": false,
  "debug.console.fontSize": 15,
  "debug.console.fontFamily": "JetBrains Mono, Input, SFMono-Regular",
  "files.associations": {
    "Brewfile": "ruby",
    "*.alfredappearance": "json"
  },
  "workbench.activityBar.visible": true,
  "files.watcherExclude": {
    "**/dist": true
  },
  "window.autoDetectColorScheme": true,
  "workbench.preferredDarkColorTheme": "Tokyo Night",
  "search.smartCase": true,
  "editor.semanticHighlighting.enabled": true,
  "workbench.preferredLightColorTheme": "Tokyo Night Light",
  "workbench.colorCustomizations": {
  },
  // "editor.tokenColorCustomizations": {
  //   "strings": "#adb5bd",
  //   "functions": "#868e96",
  //   "types": "#868e96",
  //   "keywords": "#342070",
  // },
  "workbench.iconTheme": "Rosé Pine Icons",
  "workbench.editor.limit.enabled": true,
  "workbench.editor.limit.value": 3,
  "diffEditor.renderSideBySide": false,
  "bracket-pair-colorizer-2.highlightActiveScope": true,
  "editor.formatOnSave": true,
  "prettier.trailingComma": "none",
  "prettier.semi": true,
  "prettier.arrowParens": "avoid",
  "prettier.singleQuote": false,
  "workbench.colorTheme": "Rosé Pine",
  "files.exclude": {
    "**/dist": true
  }
}
