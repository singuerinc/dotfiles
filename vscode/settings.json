{
  "editor.fontFamily": "Input, SFMono-Regular",
  "editor.renderWhitespace": "selection",
  "editor.fontWeight": "500",
  "terminal.integrated.fontWeight": "500",
  "editor.lineHeight": 22,
  "window.title": "${activeEditorShort}${separator}${activeEditorLong}",
  "editor.rulers": [],
  "files.trimTrailingWhitespace": true,
  "terminal.integrated.fontSize": 18,
  "editor.matchBrackets": true,
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "npm.packageManager": "yarn",
  "explorer.openEditors.visible": 0,
  "terminal.integrated.cursorBlinking": true,
  "typescript.updateImportsOnFileMove.enabled": "never",
  "search.quickOpen.includeSymbols": true,
  "editor.renderControlCharacters": true,
  "workbench.activityBar.visible": false,
  "git.enableSmartCommit": true,
  "git.autofetch": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "window.zoomLevel": 0,
  "workbench.statusBar.visible": false,
  "breadcrumbs.enabled": false,
  "editor.minimap.enabled": true,
  "workbench.sideBar.location": "left",
  "editor.colorDecorators": true,
  "workbench.list.keyboardNavigation": "filter",
  "editor.suggestSelection": "first",
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.suggest.localityBonus": true,
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "emmet.includeLanguages": {
    "postcss": "css"
  },
  "emmet.syntaxProfiles": {
    "postcss": "css"
  },
  "explorer.decorations.badges": false,
  "bracket-pair-colorizer-2.colors": [
    "#63e6be",
    "#8ce99a",
    "#b197fc",
    "#f06595"
  ],
  "workbench.colorCustomizations": {
    "tab.activeBackground": "#111",
    "tab.activeForeground": "#fff",
    "[Tokyo Night]": { // or "[Tokyo Night Storm]"
      "window.activeBorder": "#00ffff",
      "window.inactiveBorder":"#222"
    }
  },
  "workbench.editor.showTabs": true,
  "vim.easymotion": true,
  "vim.incsearch": true,
  "vim.leader": "\\",
  "vim.easymotionMarkerBackgroundColor": "#00FF55cc",
  "vim.easymotionMarkerWidthPerChar": 10,
  "vim.easymotionMarkerHeight": 20,
  "vim.easymotionMarkerFontFamily": "Input",
  "vim.easymotionMarkerFontSize": "12px",
  "editor.autoClosingQuotes": "always",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "emmet.showSuggestionsAsSnippets": true,
  "emmet.triggerExpansionOnTab": true,
  "gitlens.codeLens.enabled": false,
  "gitlens.views.repositories.location": "gitlens",
  "gitlens.views.fileHistory.location": "gitlens",
  "gitlens.views.lineHistory.location": "gitlens",
  "gitlens.views.compare.location": "gitlens",
  "gitlens.views.search.location": "gitlens",
  "dimmer.context": 6,
  "dimmer.opacity": 10,
  "workbench.iconTheme": "origamid-next-icons",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.fontAliasing": "antialiased",
  "terminal.integrated.fontFamily": "SFMono-Regular, Input",
  "git.untrackedChanges": "separate",
  "editor.fontSize": 18,
  "workbench.colorTheme": "Tokyo Night",
  "explorer.confirmDelete": false,
  "editor.formatOnSave": true,
  "editor.minimap.renderCharacters": false,
  "problems.showCurrentInStatus": true,
  "terminal.integrated.rendererType": "experimentalWebgl",
  "search.enableSearchEditorPreview": true,
  "terminal.external.osxExec": "Hyper.app"
}
