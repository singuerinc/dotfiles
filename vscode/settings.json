{
  "editor.fontFamily": "JetBrains Mono, Input, SFMono-Regular",
  "editor.fontSize": 17,
  "editor.renderWhitespace": "all",
  "window.title": "${activeEditorShort}${separator}${activeEditorLong}",
  "editor.rulers": [
    {
      "column": 80,
      "color": "#00FF0011"
    },
    {
      "column": 100,
      "color": "#FF990011"
    },
    {
      "column": 120,
      "color": "#FF000011"
    }
  ],
  "files.trimTrailingWhitespace": true,
  "terminal.integrated.fontSize": 15,
  "editor.matchBrackets": false,
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "npm.packageManager": "yarn",
  "explorer.openEditors.visible": 0,
  "terminal.integrated.cursorBlinking": true,
  "typescript.updateImportsOnFileMove.enabled": "never",
  "search.quickOpen.includeSymbols": true,
  "editor.renderControlCharacters": true,
  "git.enableSmartCommit": true,
  "git.autofetch": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "window.zoomLevel": 0,
  "workbench.statusBar.visible": true,
  "breadcrumbs.enabled": false,
  "editor.minimap.enabled": false,
  "editor.colorDecorators": true,
  "workbench.list.keyboardNavigation": "filter",
  "editor.suggestSelection": "first",
  "editor.wordWrap": "on",
  "editor.fontLigatures": true,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.suggest.localityBonus": true,
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "emmet.includeLanguages": {
    "postcss": "css"
  },
  "emmet.syntaxProfiles": {
    "postcss": "css"
  },
  "workbench.editor.showTabs": true,
  "vim.easymotion": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.leader": ",",
  "vim.easymotionMarkerBackgroundColor": "#00FF55cc",
  "vim.easymotionMarkerWidthPerChar": 12,
  "vim.easymotionMarkerHeight": 20,
  "vim.easymotionMarkerFontFamily": "JetBrains Mono",
  "vim.easymotionMarkerFontSize": "12px",
  "vim.easymotionMarkerYOffset": 20,
  "vim.statusBarColorControl": true,
  "vim.statusBarColors.normal": "#212529",
  "editor.autoClosingQuotes": "always",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "emmet.showSuggestionsAsSnippets": true,
  "emmet.triggerExpansionOnTab": true,
  "gitlens.codeLens.enabled": false,
  "gitlens.views.repositories.location": "gitlens",
  "gitlens.views.fileHistory.location": "gitlens",
  "gitlens.views.compare.location": "gitlens",
  "gitlens.views.search.location": "gitlens",
  "dimmer.context": 6,
  "dimmer.opacity": 10,
  "workbench.fontAliasing": "auto",
  "git.untrackedChanges": "separate",
  "explorer.confirmDelete": false,
  "editor.minimap.renderCharacters": false,
  "cSpell.userWords": [
    "Autoplay",
    "DEVTOOLS",
    "Debounced",
    "ECAS",
    "FWSUPPORT",
    "Mealplan",
    "Nahuel",
    "PAYTABLE",
    "PRODBUG",
    "Pierogi",
    "QUICKSPIN",
    "Quickspin",
    "SITEURL",
    "SPACEBAR",
    "Scotti",
    "TOPBAR",
    "Tweakpane",
    "Typestate",
    "Valentina",
    "WORDPRESS",
    "aniket",
    "animatables",
    "animejs",
    "autoplayer",
    "bugfix",
    "builtins",
    "clienthost",
    "cond",
    "customizer",
    "dashify",
    "editorconfig",
    "frontmatter",
    "garima",
    "immer",
    "jira",
    "lightgrey",
    "middlewares",
    "miragejs",
    "netent",
    "nightwatch",
    "npmrc",
    "nprogress",
    "nums",
    "placemat",
    "playui",
    "preact",
    "rgba",
    "scrollable",
    "singuerinc",
    "sourcemap",
    "stylelint",
    "swipeable",
    "tscache",
    "tslib",
    "turbolinks",
    "unmount",
    "uuidv",
    "vova",
    "woff",
    "xstate"
  ],
  "workbench.editor.highlightModifiedTabs": true,
  "editor.tabCompletion": "on",
  "problems.showCurrentInStatus": true,
  "search.enableSearchEditorPreview": true,
  "terminal.external.osxExec": "Hyper.app",
  "gitlens.blame.avatars": false,
  "editor.parameterHints.cycle": true,
  "editor.renderIndentGuides": false,
  "debug.console.fontSize": 15,
  "debug.console.fontFamily": "JetBrains Mono, Input, SFMono-Regular",
  "files.associations": {
    "Brewfile": "ruby",
    "*.alfredappearance": "json"
  },
  "workbench.activityBar.visible": false,
  "files.watcherExclude": {
    "**/dist": true,
    ".cache": true
  },
  "workbench.preferredDarkColorTheme": "Tokyo Night",
  "search.smartCase": true,
  "editor.semanticHighlighting.enabled": true,
  "workbench.iconTheme": "verdandi",
  "workbench.editor.limit.enabled": true,
  "workbench.editor.limit.value": 6,
  "diffEditor.renderSideBySide": true,
  "bracket-pair-colorizer-2.highlightActiveScope": true,
  "editor.formatOnSave": true,
  "prettier.trailingComma": "none",
  "prettier.semi": true,
  "prettier.arrowParens": "avoid",
  "prettier.singleQuote": false,
  "gitlens.views.repositories.files.layout": "list",
  "amVim.smartRelativeLineNumbers": true,
  "todo-tree.tree.showScanModeButton": false,
  "todo-tree.highlights.customHighlight": {
    "TODO": {
      "foreground": "#00FFFF"
    },
    "FIXME": {
      "foreground": "#FF0000"
    }
  },
  "git.confirmSync": false,
  "workbench.colorCustomizations": {
    "[Ros√© Pine]": {
      "editorOverviewRuler.addedForeground": "#9ccfd8ff",
      "editorOverviewRuler.border": "#0000",
      "editorOverviewRuler.bracketMatchForeground": "#706e86ff",
      "editorOverviewRuler.commonContentForeground": "#706e864d",
      "editorOverviewRuler.currentContentForeground": "#f6c17780",
      "editorOverviewRuler.deletedForeground": "#eb6f9280",
      "editorOverviewRuler.errorForeground": "#eb6f92ff",
      "editorOverviewRuler.findMatchForeground": "#706e864d",
      "editorOverviewRuler.incomingContentForeground": "#9ccfd880",
      "editorOverviewRuler.infoForeground": "#e0def4ff",
      "editorOverviewRuler.modifiedForeground": "#ebbcbaff",
      "editorOverviewRuler.rangeHighlightForeground": "#706e864d",
      "editorOverviewRuler.selectionHighlightForeground": "#706e864d",
      "editorOverviewRuler.warningForeground": "#f6c177ff",
      "editorOverviewRuler.wordHighlightForeground": "#706e864d",
      "editorOverviewRuler.wordHighlightStrongForeground": "#706e864d",

      "scrollbarSlider.activeBackground": "#706e8666",
      "scrollbarSlider.background": "#706e864d",
      "scrollbarSlider.hoverBackground": "#706e864d"
    }
  },
  "terminal.integrated.lineHeight": 1.2,
  "terminal.integrated.fontFamily": "Menlo",
  "editor.semanticTokenColorCustomizations": {
    "enabled": true, // enable semantic highlighting for all themes
    "rules": {
      // different color for all constants
      "property.readonly": "#777",

      // make all symbol declarations bold
      "*.declaration": { "bold": true }
    }
  },
  "files.exclude": {
    "**/reports": true,
    ".cache": true
  },
  "files.associations": {
    ".releaserc": "json"
  },
  "hediet.vscode-drawio.local-storage": "",
  "workbench.preferredLightColorTheme": "Visual Studio Light",
  "bracket-pair-colorizer-2.colors": ["#555", "#777", "#999"],
  "typescript.suggest.paths": false,
  "workbench.colorTheme": "GitHub Dark"
}
