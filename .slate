# This is the default .slate file.
# If no ~/.slate file exists this is the file that will be used.

config defaultToCurrentScreen true

# Window Hints
# bind esc:cmd hint

alias move2center move screenOriginX+((screenSizeX/2)-(windowSizeX/2));screenOriginY+((screenSizeY/2)-(windowSizeY/2)) newWindowSizeX;newWindowSizeY

alias num1 corner top-left resize:screenSizeX*0.6;screenSizeY
alias num2 move screenOriginX;screenOriginY+(screenSizeY/2) screenSizeX;screenSizeY/2
alias num3 corner bottom-right resize:screenSizeX/2;screenSizeY/2
alias num4 corner top-left resize:screenSizeX/2;screenSizeY
alias num5 move screenOriginX+(screenSizeX/12);screenOriginY+75 (screenSizeX)-(screenSizeX/6);screenSizeY-150
alias num6 corner top-right resize:screenSizeX/2;screenSizeY
alias num7 move screenOriginX+(screenSizeX/12);screenOriginY (screenSizeX)-(screenSizeX/6);screenSizeY
alias num8 corner top-left resize:screenSizeX;screenSizeY/2
alias num9 corner top-right resize:screenSizeX/2;screenSizeY/2
alias num0 move screenOriginX;screenOriginY screenSizeX;screenSizeY

bind f13:cmd,shift,up move screenOriginX+((screenSizeX/2)-720);screenOriginY+((screenSizeY/2)-412) 1440;824
alias f14 move screenOriginX;screenOriginY (screenSizeX/3);screenSizeY
alias f15 move screenOriginX+(screenSizeX/3);screenOriginY (screenSizeX)-(screenSizeX/3);screenSizeY
alias f16 move screenOriginX+(screenSizeX/6);screenOriginY+125 (screenSizeX)-(screenSizeX/3);screenSizeY-400
alias f17 move screenOriginX;screenOriginY+75 screenSizeX;screenSizeY-150
alias f18 move screenOriginX+(screenSizeX*.275);screenOriginY+10 screenSizeX*.45;screenSizeY-20
alias f19 move screenOriginX+(screenSizeX/6);screenOriginY+75 (screenSizeX)-(screenSizeX/3);screenSizeY-150

alias home corner top-left resize:screenSizeX/2;screenSizeY/2
alias end corner bottom-left resize:screenSizeX/2;screenSizeY/2

bind pad1:cmd,shift,up ${num1}
bind 1:cmd,shift,up ${num1}
bind z:cmd,shift,up ${num1}

bind pad2:cmd,shift,up ${num2}
bind 2:cmd,shift,up ${num2}
bind x:cmd,shift,up ${num2}

bind pad3:cmd,shift,up ${num3}
bind 3:cmd,shift,up ${num3}
bind c:cmd,shift,up ${num3}
bind pageDown:cmd,shift,up ${num3}

bind pad4:cmd,shift,up ${num4}
bind 4:cmd,shift,up ${num4}
bind a:cmd,shift,up ${num4}

bind pad5:cmd,shift,up ${num5}
bind 5:cmd,shift,up ${num5}
bind s:cmd,shift,up ${num5}

bind pad6:cmd,shift,up ${num6}
bind 6:cmd,shift,up ${num6}
bind d:cmd,shift,up ${num6}

bind pad7:cmd,shift,up ${num7}
bind 7:cmd,shift,up ${num7}
bind q:cmd,shift,up ${num7}

bind pad8:cmd,shift,up ${num8}
bind 8:cmd,shift,up ${num8}
bind w:cmd,shift,up ${num8}

bind pad9:cmd,shift,up ${num9}
bind 9:cmd,shift,up ${num9}
bind e:cmd,shift,up ${num9}
bind pageUp:cmd,shift,up ${num9}

bind pad0:cmd,shift,up ${num0}
bind 0:cmd,shift,up ${num0}
bind space:cmd,shift,up ${num0}

bind home:cmd,shift,up ${home}
bind end:cmd,shift,up ${end}

bind =:cmd,shift,up sequence resize +50 +50 | ${move2center}
bind -:cmd,shift,up sequence resize -50 -50 | ${move2center}

bind f19:cmd,shift,up ${f19}
bind f18:cmd,shift,up ${f18}
bind f17:cmd,shift,up ${f17}
bind f16:cmd,shift,up ${f16}
bind f15:cmd,shift,up ${f15}
bind f14:cmd,shift,up ${f14}

bind f12:cmd,shift,up ${f19}
bind f11:cmd,shift,up ${f18}
bind f10:cmd,shift,up ${f17}


bind 1:ctrl grid padding:1 1920x1200:6,9 1680x1050:9,6
bind n:cmd,shift,up throw next resize
